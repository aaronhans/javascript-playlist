{
  "manifest_version": 2,
  "name": "Collaborative Playlisting",
  "description": "Playlist interaction popup",
  "version": "0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "webNavigation",
    "contextMenus",
    "tabs","http://*/*","https://*/*"
  ],
  "browser_action": {
    "default_icon": "reverb.png",
    "default_popup": "index.html"
  }
}